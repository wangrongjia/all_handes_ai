<import src="../../../common/head.ttml" />
<import src="../../../common/foot.ttml" />

<view class="container">
  <template is="head" data="{{title: 'makePhoneCall'}}"/>

  <view class="page-body">
    <view class="page-section">
      <view class="desc">{{enter_phone_number}}</view>
      <input class="input" type="number" name="input" bindinput="bindInput" />
      <view class="btn-area">
        <button type="primary" bindtap="makePhoneCall" disabled="{{disabled}}">{{call}}</button>
      </view>
    </view>
  </view>

  <template is="foot" />
</view>
